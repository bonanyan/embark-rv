    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x132e3b230_0, 0, 1;
T_7.4 ;
    %load/vec4 v0x132e3b2c0_0;
    %addi 4, 0, 32;
    %store/vec4 v0x132e3b2c0_0, 0, 32;
    %jmp T_7.2;
T_7.3 ;
    %delay 2, 0;
    %load/vec4 v0x132e3b230_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_7.7, 4;
    %vpi_call 3 53 "$display", "\012============Test Passed============\012" {0 0 0};
    %jmp T_7.8;
T_7.7 ;
    %vpi_call 3 55 "$display", "\012============Test Failed============\012" {0 0 0};
T_7.8 ;
    %delay 2, 0;
    %vpi_call 3 56 "$finish" {0 0 0};
    %end;
    .thread T_7;
